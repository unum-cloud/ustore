        {
            "name": "C++: Test STL",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/ukv_stl_unit_test",
            "args": [
                "--gtest_break_on_failure",
                "--gtest_catch_exceptions=0"
            ],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "ASAN_OPTIONS",
                    "value": "detect_leaks=0:atexit=1:strict_init_order=1:strict_string_checks=1"
                }
            ],
            "showDisplayString": true,
            "stopAtEntry": false,
            "externalConsole": false,
            "preLaunchTask": "Build Native: Debug STL Test",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb"
